<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.gemery.groupradioaddfragment">
 <!--网络请求 权限管理-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
<!--本地持久数据的储存读写-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="26" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="com.example.gemery.ssww.AppExample"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:replace="android:icon,android:name,android:label,android:theme"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.gemery.ssww.MainActivity" android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        </activity>
        <activity
            android:name="com.example.gemery.ssww.activities.WelcomeActivity"

            android:theme="@style/AppTheme"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.example.gemery.ssww.activities.SignUpActivity">

        </activity>
        <activity android:name="com.example.gemery.ssww.activities.SignInActivity">

        </activity>
        <activity android:name="com.example.gemery.ssww.activities.KnowledgeActivity">
        </activity>
        <activity android:name="com.example.gemery.ssww.activities.ListActivity">
        </activity>
        <activity android:name="com.example.gemery.ssww.activities.LinkedActivity">
        </activity>
        <activity android:name="com.example.gemery.ssww.activities.OrderingInfoActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">


        </activity>

        <service android:name="com.example.gemery.ssww.service.MsfService">

        </service>

        <activity android:name="com.example.gemery.ssww.activities.ContactActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <activity android:name="com.example.gemery.ssww.activities.SendMessageActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <activity android:name="com.example.gemery.ssww.activities.CityActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            >

        </activity>
        <activity android:name="com.example.gemery.ssww.activities.QueryActivity"
            android:icon="@mipmap/ic_launcher"  android:label="@string/app_name"
            android:logo="@drawable/kccx"></activity>

        <activity android:name="com.example.gemery.ssww.activities.PersonActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>

        <activity android:name="com.example.gemery.ssww.activities.ProductListActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        </activity>
    </application>

</manifest>